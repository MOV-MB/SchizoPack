models/players/frenzy/head
{
	q3map_nolightmap
	q3map_onlyvertexlighting
    {
        map models/players/frenzy/head
        rgbGen lightingDiffuse
    }
    {
        map gfx/effects/city2_inv
        blendFunc GL_DST_COLOR GL_SRC_COLOR
        tcGen environment
    }
    {
        map models/players/frenzy/head_spec
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
}

