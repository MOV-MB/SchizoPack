models/players/Ultron_Mark_I/Ultron_Mark_I_Body_D
{
    {
        map models/players/Ultron_Mark_I/Ultron_Mark_I_Body_D
        rgbGen lightingDiffuse
    }
    {
        map models/players/Ultron_Mark_I/Ultron_Mark_I_Body_D
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
	    {
        map models/players/Ultron_Mark_I/spec1
        blendFunc GL_DST_COLOR GL_SRC_COLOR
        tcGen environment
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
			    {
		animmap 0.76 models/players/Ultron_Mark_I/Ultron_Mark_I_Body_Glow.tga models/players/Ultron_Mark_I/Ultron_Mark_I_Body_Glow.tga
        blendFunc GL_ONE GL_ONE
        detail
	}
}

