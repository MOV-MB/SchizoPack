models/players/Jedi_Godzilla/body
{
	cull	disable
    {
        map models/players/Jedi_Godzilla/body
        alphaFunc GE128
        depthWrite
        rgbGen lightingDiffuse
    }
    {
        map models/players/Jedi_Godzilla/Body_S
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
}

