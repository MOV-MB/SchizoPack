models/players/DavyJones/Algaes
{
    cull disable
    {
        map models/players/DavyJones/Algaes
        alphaFunc GE128
        rgbGen lightingDiffuse
    }
    {
        map models/players/DavyJones/Algaes
        blendFunc GL_SRC_ALPHA GL_ONE
        alphaGen lightingSpecular
        rgbGen lightingDiffuse
        detail
    }
}

