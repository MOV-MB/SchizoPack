models/players/rorschach/head
{
    {
        map models/players/rorschach/head
        blendFunc GL_ONE GL_ZERO
        rgbGen lightingDiffuse
    }
    {
        animMap 0.1 models/players/rorschach/atb.tga models/players/rorschach/btc.tga models/players/rorschach/ctd.tga models/players/rorschach/dte.tga models/players/rorschach/etf.tga models/players/rorschach/ftg.tga models/players/rorschach/gta.tga 
        alphaFunc GE128
        blendFunc GL_ONE GL_ZERO
        rgbGen lightingDiffuse
        alphaGen wave inversesawtooth 0.4 0.6 0 0.1
    }
    {
        animMap 0.1 models/players/rorschach/bfa.tga models/players/rorschach/cfb.tga models/players/rorschach/dfc.tga models/players/rorschach/efd.tga models/players/rorschach/ffe.tga models/players/rorschach/gff.tga models/players/rorschach/afg.tga 
        alphaFunc GE128
        blendFunc GL_ONE GL_ZERO
        rgbGen lightingDiffuse
        alphaGen wave sawtooth 0.4 0.6 0 0.1
    }
    {
        map models/players/rorschach/head_spec
        blendFunc GL_SRC_ALPHA GL_ONE
        alphaGen lightingSpecular
    }
}

