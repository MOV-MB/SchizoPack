models/players/kobold/mouth_eyes
{
	cull	disable
    {
        map models/players/kobold/mouth_eyes
        alphaFunc GE128
        depthWrite
        rgbGen lightingDiffuse
    }
}

models/players/kobold/body
{
    {
        map models/players/kobold/body
        rgbGen lightingDiffuse
    }
    {
        map models/players/kobold/body
        blendFunc GL_ONE GL_ONE
        glow
        rgbGen identity
    }
    {
        map models/players/kobold/body_pn
        alphaFunc GE192
        depthWrite
        rgbGen lightingDiffuse
    }
    {
        map models/players/kobold/body_spec
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
}

