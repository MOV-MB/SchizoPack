models/players/DrOctopus_Marvel/tentacles
{
	cull	disable
    {
        map models/players/DrOctopus_Marvel/tentacles
        rgbGen lightingDiffuse
    }
    {
        map models/players/DrOctopus_Marvel/tentacles_S
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
	    {
        map models/players/DrOctopus_Marvel/spec1
        blendFunc GL_DST_COLOR GL_SRC_COLOR
        tcGen environment
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
    {
        map models/players/DrOctopus_Marvel/tentacles_glow
        blendFunc GL_ONE GL_ONE
        glow
        rgbGen identity
    }
}

