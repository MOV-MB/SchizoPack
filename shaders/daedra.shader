models/players/Daedra/armor
{
	q3map_nolightmap
	cull	twosided
    	{
       		map models/players/Daedra/armor
       		blendFunc GL_ONE GL_ZERO
        	rgbGen lightingDiffuse
    	}
	{
        	map models/players/Daedra/armor_spec
        	blendFunc GL_SRC_ALPHA GL_ONE
        	detail
        	alphaGen lightingSpecular
    	}
		{
		        map models/players/Daedra/armor-glow
        blendFunc GL_ONE GL_ONE
        glow
    }
}

