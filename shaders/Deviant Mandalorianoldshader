models/players/DeviantMando/Torso5
{
       	{
		map models/players/DeviantMando/Torso5
                blendFunc GL_ONE GL_ZERO
                rgbGen lightingDiffuse
	}
	{
		map GFX/Effects/Envchrome
                blendFunc GL_DST_COLOR GL_SRC_COLOR
                tcGen environment
	}
	{
		map models/players/DeviantMando/Torso5_spec
                blendFunc GL_SRC_ALPHA GL_ONE
                detail
                alphaGen lightingSpecular
	}
}

models/players/DeviantMando/gogs4
{        
	{
		map models/players/DeviantMando/gogs4
		rgbGen lightingDiffuse
	}
	{
		map GFX/Effects/Envchrome2
		blendfunc add
		rgbGen lightingDiffuse
		tcGen environment 
	}
	{
                map GFX/Effects/Saber_glow
                blendFunc GL_SRC_ALPHA GL_SRC_ALPHA
                tcMod scroll 3 0.5
	}
	{
		map models/players/DeviantMando/gogs4
		blendfunc blend
		rgbGen lightingDiffuse
	}
	{
		map GFX/Effects/Envchrome
                blendFunc GL_DST_COLOR GL_SRC_COLOR
                tcGen environment
	}
	{
		map models/players/DeviantMando/gogs4
                blendFunc GL_SRC_ALPHA GL_ONE
                detail
                alphaGen lightingSpecular
	}
        {
                map models/players/DeviantMando/gogs3_glow
                blendFunc GL_ONE GL_ONE
                glow
                rgbGen identity
        }
}

