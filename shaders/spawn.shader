models/players/spawn/face
{
	q3map_nolightmap
    {
        map models/players/spawn/face
    }
    {
        map models/players/spawn/face_circuit
        blendFunc GL_SRC_ALPHA GL_DST_ALPHA
        rgbGen wave noise 0.3 0.1 0 5
    }
}

models/players/spawn/caps
{
	q3map_nolightmap
    {
        map models/players/spawn/caps
        alphaFunc GE192
        depthWrite
        rgbGen lightingDiffuse
    }
    {
        map models/players/spawn/caps
        blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
        rgbGen lightingDiffuse
    }
}

models/players/spawn/head_default
{
	q3map_nolightmap
    {
        map models/players/spawn/head_default
        rgbGen lightingDiffuse
    }
    {
        map gfx/effects/chr_white_add_mild
        blendFunc GL_SRC_ALPHA GL_ONE
        depthFunc equal
        detail
        tcGen environment
    }
}

