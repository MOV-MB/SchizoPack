models/players/Professor_X/Body
{
	cull	twosided
    {
        map models/players/Professor_X/Body
        rgbGen lightingDiffuse
        //alphaFunc GE128
    }
    {
        map models/players/Professor_X/Body_S
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
}

