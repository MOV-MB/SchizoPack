models/players/Mister_Fantastic/Body
{	  
	cull	disable
    {
        map models/players/Mister_Fantastic/Body
        blendFunc GL_ONE GL_ZERO
        rgbGen lightingDiffuse
    }
    {
        map models/players/Mister_Fantastic/Body_S
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
}

