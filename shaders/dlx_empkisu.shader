models/players/dlx_empkisu/eyes
{
    {
        map models/players/dlx_empkisu/eyes
    }
    {
        map models/players/dlx_empkisu/eyes_spec
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
}

