models/players/dlx_empkisu/feather
{
    {
        map models/players/dlx_empkisu/feather
        blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
    }
    {
        map models/players/dlx_empkisu/feather_env
        blendFunc GL_DST_COLOR GL_SRC_COLOR
        tcGen environment
    }
    {
        map models/players/dlx_empkisu/spec
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
