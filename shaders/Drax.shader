models/players/Drax/Body
{
	cull	twosided
    {
        map models/players/Drax/Body
        rgbGen lightingDiffuse
        //alphaFunc GE128
    }
    {
        map models/players/Drax/Body_S
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
}

