models/players/superman/cape
{
    {
        map models/players/superman/cape
        rgbGen lightingDiffuse
    }
	{
        map models/players/superman/cape_shine
        blendFunc GL_SRC_ALPHA GL_ONE
        alphaGen lightingSpecular
	detail
    	}

}

