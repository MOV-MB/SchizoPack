models/players/pskjhd/hand_paint
{
	q3map_nolightmap
    {
        map models/players/pskjhd/hand_paint
        blendFunc GL_ONE GL_ZERO
        rgbGen lightingDiffuse
    }
    {
        map models/players/pskjhd/default_reflect
        blendFunc GL_ONE GL_ONE
        rgbGen lightingDiffuse
        tcGen environment
    }
    {
        map models/players/pskjhd/hand_paint
        blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
        rgbGen lightingDiffuse
    }
    {
        map models/players/pskjhd/hand_glow
        blendFunc GL_ONE GL_ONE
        detail
    }
}

