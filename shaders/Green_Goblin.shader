models/players/Green_Goblin_Raimi/body
{
	cull	twosided
    {
        map models/players/Green_Goblin_Raimi/body
        rgbGen lightingDiffuse
        //alphaFunc GE128
    }
    {
        map models/players/Green_Goblin_Raimi/body
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
	    {
        map models/players/Green_Goblin_Raimi/spec1
        blendFunc GL_DST_COLOR GL_SRC_COLOR
        tcGen environment
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
    {
        map models/players/Green_Goblin_Raimi/Body_glow
        blendFunc GL_ONE GL_ONE
        glow
        rgbGen identity
    }

}

